{
  "name": "lethean",
  "version": "1.2.2",
  "description": "",
  "author": "Snider<snider@lethean.io>",
  "private": true,
  "license": "GPLv3",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:sdk": "sh updateVersion.sh && bash sdk/build.sh",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "pm2 start pm2.json",
    "stop": "pm2 stop pm2.json",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "setup:macos": "bash sdk/install.deps.macos.sh",
    "setup:db": "node data/seed-db.js"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.8",
    "@types/rethinkdb": "^2.3.17",
    "@types/xml2js": "^0.4.9",
    "axios": "^0.21.1",
    "cache-manager": "^3.4.4",
    "fast-xml-parser": "^3.19.0",
    "helmet": "^4.6.0",
    "ion-js": "^4.2.1",
    "jsbi": "^3.1.5",
    "nest-openapi-tools": "^3.0.1",
    "pm2": "^5.1.0",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rethinkdb": "^2.4.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
